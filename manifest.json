{
    "manifest_version": 3,
    "name": "GitHub Absolute Time Plugin",
    "version": "1.0",
    "description": "Display absolute time on GitHub pages instead of relative time",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://github.com/*"],
        "js": ["content.js"]
      }
    ]
  }